<Page x:Class="FitLevel_RPG.PreMadeWorkouts"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FitLevel_RPG"
      mc:Ignorable="d" 
      d:DesignHeight="368" d:DesignWidth="684"
      Title="PreMadeWorkouts">

    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Center" Margin="0,39,0,0" TextWrapping="Wrap" Text="Pre-Made Workouts" VerticalAlignment="Top" FontSize="18"/>
        <ComboBox x:Name="difficultyComboBox" HorizontalAlignment="Left" Margin="139,124,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="difficultyComboBox_SelectionChanged"/>
        <ListView x:Name="listView"  Margin="139,188,49,57">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="button" Content="Open Selected Pre-Made" HorizontalAlignment="Left" Margin="469,316,0,0" VerticalAlignment="Top" Width="166" Click="OpenPreMadeWorkout"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="139,103,0,0" TextWrapping="Wrap" Text="Workout Difficulty:" VerticalAlignment="Top"/>

    </Grid>
</Page>
