<Window x:Class="FitLevel_RPG.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FitLevel_RPG"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <Frame x:Name="Main" NavigationUIVisibility="Hidden"/>
        <TextBlock Text="Login" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="24"></TextBlock>

        <StackPanel Orientation="Horizontal" Height ="35" HorizontalAlignment="Center">
            <Button Content="Register" Click="BtnClickLRegister"></Button>
        </StackPanel>
    </Grid>
</Window>
